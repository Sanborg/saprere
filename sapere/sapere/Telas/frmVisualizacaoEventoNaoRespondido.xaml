        <Image x:Name="imgFotoEvento" HorizontalAlignment="Left" Height="100" Margin="67,248,0,0" VerticalAlignment="Top" Width="100"/>
        <TextBlock x:Name="txtDescricaoEvento" HorizontalAlignment="Left" Margin="37,388,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="111" Width="452" FontFamily="Lucida Sans" FontWeight="Bold" TextAlignment="Center" Padding="0,0,380,0" MouseLeftButtonDown="RealizarLogin"/>
